<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="s_container_3d" name="Container 3d">
    <section class="pt32 pb32">
      <div class="container">
        <div class="row">
          <div class="container-3d"/>
        </div>
      </div>
    </section>
  </template>

  <template id="snippets_3d" inherit_id="website.snippets">
    <xpath expr="//div[@id='snippet_structure']" position="before">
      <div id="snippet_example" class="o_panel">
          <div class="o_panel_header">
              <i class="fa fa-th-large"/> Example
          </div>
          <div class="o_panel_body">
            <t t-snippet="theme_example.s_container_3d" t-thumbnail="/theme_example/static/src/img/s_container_3d.png"/>
          </div>
      </div>
    </xpath>

  </template>
</odoo>
